@startuml
package "Огляд прецеденту" {
    actor "Адміністратор" as Admin
    usecase "Налаштування підсистеми" as SE_7 #eeffee
    Admin -u-> SE_7
}


usecase "Перегляд налаштувань" as SE_7.1
usecase "Додати джерело" as SE_7.2
usecase "Видалити джерело" as SE_7.3
SE_7.1 ..> SE_7:includes
SE_7.2 ..> SE_7:extends
SE_7.3 ..> SE_7:extends
@enduml
